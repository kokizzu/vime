{
  "name": "@vime/vue",
  "version": "1.0.0",
  "description": "Collection of vue components to build your own media player.",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "contributors": [
    "Rahim Alwer <rahim_alwer@outlook.com>"
  ],
  "bugs": {
    "url": "https://github.com/vime-js/vime/issues"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:cjs && npm run build:esm && npm run copy:css",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm",
    "copy:css": "cp-cli ../core/dist/vime/vime.css dist/vime.css",
    "release": "semantic-release -e ../../tools/semantic-release"
  },
  "dependencies": {
    "@vime/core": "^1.0.0"
  },
  "devDependencies": {
    "cp-cli": "^2.0.0",
    "rimraf": "^3.0.2",
    "vue": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vime",
    "player",
    "media",
    "vue",
    "audio",
    "video",
    "html5",
    "stenciljs",
    "youtube",
    "dailymotion",
    "vimeo",
    "hls",
    "dash",
    "custom"
  ]
}
