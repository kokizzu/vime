{
  "name": "@vime-js/complete",
  "description": "Vime Complete Player",
  "version": "1.6.5",
  "svelte": "src/index.js",
  "main": "dist/complete.js",
  "module": "dist/complete.esm.js",
  "unpkg": "dist/complete.js",
  "jsdelivr": "dist/complete.js",
  "sideEffects": false,
  "license": "MIT",
  "author": "Rahim Alwer <rahim_alwer@hotmail.com>",
  "homepage": "https://github.com/vime-js/vime#readme",
  "repository": "https://github.com/vime-js/vime/tree/master/packages/vime-complete",
  "bugs": {
    "url": "https://github.com/vime-js/vime/issues"
  },
  "keywords": [
    "vime",
    "audio",
    "video",
    "player",
    "complete",
    "youtube",
    "dailymotion",
    "vimeo",
    "html5",
    "file",
    "lazy",
    "lightweight",
    "fast",
    "embed",
    "hls",
    "dash",
    "plugins",
    "preconnections",
    "multi-provider",
    "cross-browser"
  ],
  "files": [
    "dist",
    "src",
    "static"
  ],
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "svg:min": "svgo --config ./svgo.yml -f static/icons",
    "svg:sprite": "svgstore --inline -o static/sprite.svg static/icons/*.svg"
  },
  "dependencies": {
    "@vime-js/core": "^1.6.4",
    "@vime-js/lite": "^1.6.4",
    "@vime-js/standard": "^1.6.4",
    "@vime-js/utils": "^1.6.4"
  },
  "devDependencies": {
    "svgo": "^1.3.2",
    "svgstore-cli": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
