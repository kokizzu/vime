{
  "name": "@vime/angular-example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "serve": "npm run serve:media & ng serve",
    "serve:media": "sirv node_modules/@vime/media --cors --port 3335 --quiet",
    "setup": "npm run setup:install && npm run setup:build",
    "setup:install": "pnpm install --filter @vime/core --filter @vime/angular --filter @vime/angular-example",
    "setup:build": "pnpm run build --filter @vime/core && pnpm run build --filter @vime/angular"
  },
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@vime/core": "^1.0.0",
    "@vime/angular": "^1.0.0",
    "@vime/media": "^1.0.0",
    "rxjs": "~6.5.5",
    "sirv-cli": "^1.0.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/jest": "^26.0.9",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jest-diff": "^26.4.0",
    "pretty-format": "^26.4.0",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  }
}
