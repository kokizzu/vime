{
  "name": "@vime/vue-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "serve": "npm run serve:media & vue-cli-service serve",
    "serve:media": "sirv node_modules/@vime/media --cors --port 3335 --quiet",
    "setup": "npm run setup:install && npm run setup:build",
    "setup:install": "pnpm install --filter @vime/core --filter @vime/vue --filter @vime/vue-example",
    "setup:build": "pnpm run build --filter @vime/core && pnpm run build --filter @vime/vue"
  },
  "dependencies": {
    "@vime/core": "^1.8.1",
    "@vime/media": "^1.0.0",
    "@vime/vue": "^1.3.0",
    "sirv-cli": "^1.0.6",
    "tslib": "^2.0.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^8.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-typescript": "~4.5.4",
    "@vue/cli-service": "~4.5.4",
    "typescript": "~4.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
